{
  "builds": [
    {
      "src": "src/main.jsx",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://chatlore-api.onrender.com/api/$1"
    }
  ]
  {
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://chatlore-api.onrender.com/api/$1"
    }
  ]
}
}
